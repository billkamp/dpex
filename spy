# Executed by interactive Python interpreter on startup.
# Pointed to by $PYTHONSTARTUP
# execute stpy to use this file in py.
import readline
def history():
 for i in range(1, readline.get_current_history_length()+1):
  print('%3d %s' % (i, readline.get_history_item(i)))

def is_it_true(anything):
   if anything:
     print("yes, it's true")
   else:
     print("no, it's false")
     
